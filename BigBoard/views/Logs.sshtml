@Master['MasterPage']


@Section['Content']

<table class="table table-striped">
    <thead>
        <tr>
            <th>ID</th>
            <th>Date</th>
            <th>Message</th>
        </tr>
    </thead>
    <tbody>
@Each.Logs 
    <tr>
        <td>@Current.ID</td>
        <td>@Current.Date</td>
        <td>@Current.Message</td>
    </tr>
@EndEach
    </tbody>
</table>
@EndSection